@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1 class="col-md-4 offset-md-2">Users</h1>


<table class="table col-md-6 offset-md-3">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
               @item.Id
            </td>
            <td>
                @item.UserName
            </td>
            <td>
              @item.Email
            </td>
            
        </tr>
}
    </tbody>
</table>
