@model IEnumerable<Page>

    @foreach (var item in Model)
    {
        @if(item.Slug != "home")

       { <li class="nav-item">
            <a class="nav-link text-dark" style="font-weight:bolder" asp-area="" asp-controller="Pages" asp-action="Page" 
            asp-route-slug="@item.Slug">@item.Title </a>
 
            </li>

       }


    }
   
